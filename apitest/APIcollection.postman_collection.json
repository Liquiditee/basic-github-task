		{
			"name": "Get All Todo Entries",
			"item": [
				{
					"name": "Get All Todo Entries",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();\r",
									"console.log(jsonData.categories[1]);\r",
									"console.log(jsonData.apiVersion);\r",
									"\r",
									"pm.environment.set(\"categories\",jsonData.categories[1])\r",
									"pm.environment.set(\"apiVersion\",jsonData.apiVersion)\r",
									"\r",
									"\r",
									"\r",
									"\r",
									"pm.test(\"Status code should be 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"pm.test(\"Status code name should have a OK string\", function () {\r",
									"    pm.response.to.have.status(\"OK\");\r",
									"});\r",
									"var jsonData = pm.response.json();\r",
									"console.log(jsonData)\r",
									"console.log(jsonData.apiVersion)\r",
									"console.log(jsonData.categories[1])\r",
									"console.log(jsonData.links[3].href)\r",
									"\r",
									"pm.test(\"confirm that the entry apiVersion is 7.1.4.0\", function () {\r",
									"    pm.expect(jsonData.apiVersion).to.eql(\"7.1.4.0\");\r",
									"});\r",
									"pm.test(\"confirm that the categories[1]) 7.1.4.0\", function () {\r",
									"    pm.expect(jsonData.categories[1]).to.eql(\"system\");\r",
									"});\r",
									"pm.test(\"Body matches string\", function () {\r",
									"    pm.expect(pm.response.text()).to.include(\"Fusio\")\r",
									"});\r",
									"\r",
									"pm.test(\"confirm that the entry  with links[3].href   title is  https://demo.fusio-project.org/system/route\",function () {\r",
									"    pm.expect(jsonData.links[3].href ).to.eql(\"https://demo.fusio-project.org/system/route\");\r",
									"});\r",
									"\r",
									"\r",
									"\r",
									"\r",
									""
